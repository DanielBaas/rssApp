{
  "manifest_version": 2,

  "name": "First Chrome Extension",
  "description": "Send Url On Click",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "content_scripts": [
    {
    "matches": ["http://*/*"],
    "js": ["jquery.js"]
    }
  ],

  "permissions": [
  "activeTab",
  "http://localhost/Rss", 
  "https://localhost/Rss"
  ]
}